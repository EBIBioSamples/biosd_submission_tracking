<%= render_partial "title" %>

<div>
  <% form_tag :action => "update", :id => @experiment.id do %>
    <%= render_partial "form" %>
    <%= hidden_field_tag("page", params[:page]) %>
    <%= hidden_field_tag("search_term", params[:search_term]) %>
    <%= content_tag("p", submit_tag("Update")) %>
    <%= render_partial "annotate" %>
    <%= content_tag("p", submit_tag("Update")) %>
  <% end %>
</div>